<h1>Your Games</h1><table>  <tr>    <th>Game Players</th>        <th></th>  </tr><%game_check = false%><% @games.each do |game| %>  <tr>    <td style="text-align:center;">	    <b>			<%if @player.id == game.player1_id%>				<%= game.player1.name %>			<%else%>				<%= game.player2.name %>			<%end%>		</b>		<i>vs</i>		<b>			<%if @player.id == game.player1_id%>				<%= game.player2.name %>			<%else%>				<%= game.player1.name %>			<%end%>		</b>	</td>	    <td style="text-align:center;">    	<% if !game.game_status %>			<%if !game_check%>    			<b><%= link_to 'Play', controller: :players, action: :mood_measure_form, :id => game.id, :start_end_check => "start" %></b>    			<%game_check = true%>    		<%else%>    			Game not initialized    		<%end%>    		    	<%else%>    		Game complete    	<%end%>    </td>  </tr><% end %></table><%#= link_to 'New Player', new_player_path %>