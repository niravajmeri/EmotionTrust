<h1>Your Games</h1><table>  <tr>    <th>You</th>    <th>Opponent</th>    <th>Game Status</th>    <th></th>    <th></th>  </tr><% @games.each do |game| %>  <tr>    <td>		<%if @player.id == game.player1_id%>			<%= game.player1.name %>		<%else%>			<%= game.player2.name %>		<%end%>	</td>		<td>		<%if @player.id == game.player1_id%>			<%= game.player2.name %>		<%else%>			<%= game.player1.name %>		<%end%>	</td>	    <td><%= game.game_status %></td>    <td><%= link_to 'Play', controller: :players, action: :mood_measure_form, :id => game.id, :start_end_check => "start" %></td>  </tr><% end %></table><%#= link_to 'New Player', new_player_path %>