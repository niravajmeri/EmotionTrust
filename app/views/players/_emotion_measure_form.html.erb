<%= form_tag :controller => :players, :action=> :emotion_measure do%>    <h2>Hey! How are you feeling now?</h2>	<%=image_tag "happy_sad_faces.jpg" %> <br/>    		<%@emotions.each do|emotion|%>		<%=radio_button_tag "emotion", emotion.id%> <%=emotion.name%>	<%end%>	<%= hidden_field_tag :id, @game_round.id%> <br/>    <%= submit_tag "Submit"%><%end%>