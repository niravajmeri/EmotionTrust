<div style="text-align: center; margin: 0 auto; margin-top: 20px;">	<%= form_tag :controller => :players, :action=> :mood_measure do%>    <h2>How are you feeling now!</h2> 		<%=image_tag "happy_sad_faces.jpg" %> <br/>		<%@moods.each do|mood|%>		<%=radio_button_tag "mood", mood.id%> <%=mood.name%>	<%end%>				<%=hidden_field_tag :start_end_check, @start_end_check%>	<%= hidden_field_tag :id, @game.id%> <br/>    <%= submit_tag "Submit", :id => @game.id%><%end%></div>