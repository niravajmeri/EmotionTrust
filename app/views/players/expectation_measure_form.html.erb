<div style="text-align: center; margin: 0 auto; margin-top: 20px;">	<%= form_tag :controller => :players, :action=> :expectation_measure do%>    <h2>Do you expect your opponent to cooperate or defect?</h2>	<%=image_tag "cooperation.jpg", :style => "width: 125px; height: 125px;" %> <br/>		<%@expectations.each do|expectation|%>		<%=radio_button_tag "expectation", expectation.id%> <%=expectation.name%>	<%end%>	<br/>	<%= hidden_field_tag :id, @game_round.id%>    <%= submit_tag "Submit"%><%end%></div>